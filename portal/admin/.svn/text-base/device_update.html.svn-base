<script type="text/javascript">

$(document).ready(function () 
	{

// IS_ACTIVE, DISPLAY_ID, IS_GRAPHICS_ACCEL, MAKE, MODEL, MODEL_VERSION, NAME, ORIENTATION, PIXELS_PER_INCH, IS_TOUCH, UA_PROFILE, USER_AGENT

// var jsondata = '{        "active": true,      "graphicsAccel": null,        "make": "TestHTC",        "model": "TestAria",        "modelVersion": null,        "name": "HTCAria",        "orientation": null,        "pixelsPerInch": null,        "runtimes": null,        "touch": null,        "uaProfile": null,        "userAgent": null    }';

var jsondata = '{         "make": "MBTestHTC2",        "model": "MBTestAria2"}';


$.ajax({
  url: getBaseURL() + getAdminAppURL() + '/devices/10008/',
  type:'POST',
contentType:'application/json',
  processData: false,
  data: jsondata,

  success: function(data) {
    $('.result').html(data);
    alert('Load was performed.');
  }
});


	$.getJSON(getBaseURL() + getAdminAppURL() + '/devices/10001', function(data)
{




		$('#element_1').val(data.content.model);
		$('#element_2').val(data.content.name);
		$('#element_3').val(data.content.modelVersion);
		$('#element_4').val(data.content.userAgent);
		$('#element_5').val(data.content.bmp_url);
		$('#element_6').val(data.content.pixelsPerInch);

		$('#element_8').val(data.content.msm);
		$('#element_9').val(data.content.ram);
		$('#element_10').val(data.content.rom);
		$('#element_11').val(data.content.filespace);

		$('#element_21').val(data.content.displaySize);
		$('#element_22').val(data.content.secondaryDisplaySize);



		$("select#element_13 option")
		   .each(function() { this.selected = (this.text == data.content.make); });

		$("select#element_14 option")
		   .each(function() { this.selected = (this.text == data.content.extensionsupp); });

		$("select#element_15 option")
		   .each(function() { this.selected = (this.text == data.content.aspectRatio); });

		$("select#element_16 option")
		   .each(function() { this.selected = (this.text == data.content.deviceStatus); });

		$("select#element_19 option")
		   .each(function() { this.selected = (this.text == data.content.accelerometer); });

		$("select#element_23 option")
		   .each(function() { this.selected = (this.text == data.content.resolution); });

		$("select#element_24 option")
		   .each(function() { this.selected = (this.text == data.content.orientation); });

		$("select#element_25 option")
		   .each(function() { this.selected = (this.text == data.content.inputInterface); });

		$("select#element_26 option")
		   .each(function() { this.selected = (this.text == data.content.graphicsAccel); });

		$("input[name='element_27'][value='"+data.content.LBS+"']").attr("checked","checked");


	}
	);
	
	});
</script>
